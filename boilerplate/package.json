{
  "name": "<%= moduleSafeName %>",
  "version": "0.0.1",
  "description": "<%= moduleDescription %>",
  "repository": {
    "type": "git",
    "url": "https://github.com/haaretz/<%= moduleSafeName %>"
  },
  "author": {
    "name": "<%= moduleAuthorName %>",
    "email": "<%= moduleAuthorEmail %>"
  },
  "license": "MIT",
  "scripts": {
    "init": "git init && npm i",
    "test": "npm run test:browser",
    "test:browser": "karma start config/karma.conf.js --no-auto-watch",
    "postinstall": "jspm install",
    "prepublish": "npm run build",
    "build": "npm run build:jspm",
    "build:jspm": "npm run clean && mkdirp dist && npm run build:jspm:dev && npm run build:jspm:prod && npm run build:jspm:es6",
    "build:jspm:dev": "jspm build src/index.js dist/<%= moduleSafeName %>.js --format global --global-name <%= moduleUppercaseName %> --source-map-contents",
    "build:jspm:prod": "jspm build src/index.js dist/<%= moduleSafeName %>.min.js --minify --source-map-contents",
    "build:jspm:es6": "jspm build src/index.js dist/<%= moduleSafeName %>.es6.js --format esm --source-map-contents",
    "lint": "eslint ./src/**/*.js",
    "lint:tests": "eslint ./src/**/*__tests__*/**/*.js",
    "lint:source": "eslint src/"
  },
  "main": "dist/<%= moduleSafeName %>.min.js",
  "jsnext:main": "src/index.js",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.0.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "browser-sync": "^2.17.5",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "del": "^2.2.2",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.0.1",
    "git-hooks": "^1.1.6",
    "isparta": "^4.0.0",
    "jsdoc": "^3.4.2",
    "jsdom": "^9.4.2",
    "jspm": "^0.17.0-beta.31",
    "karma": "^1.2.0",
    "karma-jspm": "^2.2.0",
    "loke-jsdoc-theme": "^2.1.0",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^3.3.1",
    "rollup-plugin-filesize": "^0.4.4",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-stub": "^1.1.0",
    "rollup-plugin-typescript": "^0.7.7",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-stream": "^1.11.0",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "rimraf": "^2.5.2",
    "mkdirp": "^0.5.1"
  },
  "dependencies": {},
  "jspm": {
    "name": "<%= moduleSafeName %>",
    "config": "jspm.config.js",
    "main": "src/index.js",
    "directories": {
      "lib": "src/lib"
    },
    "dependencies": {},
    "devDependencies": {
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.13"
    },
    "peerDependencies": {
      "buffer": "github:jspm/nodelibs-buffer@^0.2.0-alpha",
      "process": "github:jspm/nodelibs-process@^0.2.0-alpha"
    }
  }
}
